          <button type="submit" class="btn btn-success">Search</button>
        </form>
      </div>
      <!--/.nav-collapse -->
    </div>
    <!--/.container-fluid -->
  </nav>
  <div class="container">
    <div class="row">
      <div class="col-md-10 col-md-offset-1">
        <div id="toolbar" class="btn-group">
          <button id="btn_add" type="button" class="btn btn-default" data-toggle="modal" data-target="#Add">
            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>新增
          </button>
          <button id="btn_edit" type="button" class="btn btn-default">
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>修改
          </button>
          <button id="btn_delete" type="button" class="btn btn-default">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>删除
          </button>
        </div>
        <table id="video-table"></table>
      </div>
    </div>
  </div>
  <div class="modal fade" id="Add" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" id="myModalLabel">新增条目</h4>
        </div>
        <div class="modal-body">
          <div class="panel-body" style="padding-bottom:0px;">
            <div class="panel panel-default">
              <div class="panel-heading">条目内容</div>
              <div class="panel-body">
                <form id="formSearch" class="form-horizontal">
                  <div class="form-group" style="margin-top:15px">
                    <label class="control-label col-sm-2" for="txt_add_id">编号</label>
                    <div class="col-sm-10">
                      <input type="number" class="form-control" id="txt_add_id">
                    </div>
                    <label class="control-label col-sm-2" for="txt_add_title">标题</label>
                    <div class="col-sm-10">
                      <input type="text" class="form-control" id="txt_add_title">
                    </div>
                    <label class="control-label col-sm-2" for="txt_add_date">上传时间</label>
                    <div class="col-sm-10">
                      <div class="input-group date form_datetime" data-date="2017-01-01T00:00:00Z" data-date-format="yyyy-mm-dd hh:ii:ss" , data-link-field="txt_add_date">
                        <input size="16" type="text" class="form-control" value="" readonly>
                        <span class="input-group-addon">
                          <span class="glyphicon glyphicon-remove"></span>
                        </span>
                        <span class="input-group-addon">
                          <span class="glyphicon glyphicon-th"></span>
                        </span>
                      </div>
                      <input type="hidden" value="" id="txt_add_date">
                    </div>
                    <label class="control-label col-sm-2" for="txt_add_username">作者</label>
                    <div class="col-sm-10">
                      <input type="text" class="form-control" id="txt_add_username">
                    </div>
                    <label class="control-label col-sm-2" for="txt_add_plays">播放量</label>
                    <div class="col-sm-4">
                      <input type="number" class="form-control" id="txt_add_plays">
                    </div>
                    <label class="control-label col-sm-2" for="txt_add_comments">评论量</label>
                    <div class="col-sm-4">
                      <input type="number" class="form-control" id="txt_add_comments">
                    </div>
                    <label class="control-label col-sm-2" for="txt_add_coins">硬币数</label>
                    <div class="col-sm-4">
                      <input type="number" class="form-control" id="txt_add_coins">
                    </div>
                    <label class="control-label col-sm-2" for="txt_add_collects">收藏量</label>
                    <div class="col-sm-4">
                      <input type="number" class="form-control" id="txt_add_collects">
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
            <button type="button" class="btn btn-primary" id="add_submit">提交</button>
          </div>
        </div>
        <!-- /.modal-content -->
      </div>
      <!-- /.modal -->
    </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://cdn.bootcss.com/jquery/3.2.1/jquery.min.js"></script>
    <script type='text/javascript' , src='../public/bootstrap-stable/js/bootstrap.min.js'></script>
    <script type='text/javascript' , src='../public/bootstrap-table/bootstrap-table.min.js'></script>
    <script type='text/javascript' , src='../public/bootstrap-table/bootstrap-table-mobile.js'></script>
    <script type='text/javascript' , src='../public/bootstrap-table/bootstrap-table-zh-CN.min.js'></script>
    <script type='text/javascript' , src='../public/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js'></script>
    <script type='text/javascript' , src='../public/js/loadtable.js'></script>
    <script type='text/javascript' , src='../public/js/add.js'></script>
    <script type='text/javascript' , src='../public/js/delete.js'></script>
</body>

</html>